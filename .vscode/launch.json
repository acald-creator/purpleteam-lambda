  "version": "0.2.0",
  "configurations": [    
    
    {
      "name": "app-slave-provisioner",
      "type": "node",
      "request": "attach",
      "address": "localhost",
      "port": 5858,
      // From the sam init example, it would be "${workspaceRoot}/hello_world"
      "localRoot": "${workspaceRoot}/app-slave-provisioner",
      "remoteRoot": "/var/task",
      "protocol": "inspector",
      "stopOnEntry": false
    }, {
      "name": "selenium-hub-nodes-provisioner",
      "type": "node",
      "request": "attach",
      "address": "localhost",
      "port": 5858,
      // From the sam init example, it would be "${workspaceRoot}/hello_world"
      "localRoot": "${workspaceRoot}/selenium-standalone-provisioner",
      "remoteRoot": "/var/task",
      "protocol": "inspector",
      "stopOnEntry": false
  }]
}
